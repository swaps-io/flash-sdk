{
  "name": "@swaps-io/flash-sdk",
  "version": "1.1.0-rc.6",
  "author": "Swaps.io Team",
  "description": "Swaps.io - Flash SDK",
  "license": "MIT",
  "keywords": [
    "swaps.io",
    "flash",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swaps-io/flash-sdk.git"
  },
  "homepage": "https://github.com/swaps-io/flash-sdk",
  "bugs": {
    "url": "https://github.com/swaps-io/flash-sdk/issues"
  },
  "main": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettify": "yarn prettier --write src",
    "p": "yarn prettify",
    "prettify:test": "yarn prettier --write test",
    "pt": "yarn prettify:test",
    "clean": "rm -rf ./dist ./lib",
    "c": "yarn clean",
    "lint": "yarn eslint src",
    "l": "yarn lint",
    "compile": "yarn tsc",
    "co": "yarn compile",
    "mv_lib": "mv ./lib ./dist",
    "cp_src": "cp -r ./src ./dist",
    "build": "yarn clean && yarn lint && yarn compile && yarn mv_lib && yarn cp_src",
    "b": "yarn build",
    "test": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --verbose=true --runInBand",
    "t": "yarn test",
    "tg": "yarn test -t",
    "gen:api:main-v0": "orval --config tools/api/main-v0/config.ts",
    "gen:api:crypto-v0": "orval --config tools/api/crypto-v0/config.ts",
    "gen:api:collateral-v0": "orval --config tools/api/collateral-v0/config.ts",
    "gen:api": "yarn gen:api:main-v0 && yarn gen:api:crypto-v0 && yarn gen:api:collateral-v0",
    "ga": "yarn gen:api",
    "docs:gen": "yarn typedoc",
    "dg": "yarn docs:gen",
    "docs:open": "open ./docs/index.html",
    "do": "yarn docs:open",
    "docs": "yarn docs:gen && yarn docs:open",
    "d": "yarn docs",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@safe-global/protocol-kit": "5.0.4",
    "@tanstack/react-query": "5.62.3",
    "@trivago/prettier-plugin-sort-imports": "5.2.0",
    "@types/jest": "29.5.14",
    "@types/qs": "6.9.17",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "axios": "1.7.9",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "ethers": "6.13.4",
    "jest": "29.7.0",
    "orval": "7.3.0",
    "prettier": "3.4.2",
    "qs": "6.13.1",
    "ts-jest": "^29.2.5",
    "typedoc": "0.27.4",
    "typescript": "5.7.2",
    "viem": "2.21.54",
    "wagmi": "2.13.4"
  },
  "peerDependencies": {
    "@safe-global/protocol-kit": "5.0.4",
    "@tanstack/react-query": ">=5.17 <6",
    "axios": ">=1.6 <2",
    "ethers": ">=6.9 <7",
    "qs": ">=6.11 <7",
    "viem": ">=2.1 <3",
    "wagmi": ">=2.2 <3"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "resolutions": {
    "jsonpath-plus": "10.1.0"
  },
  "packageManager": "yarn@4.5.3"
}
