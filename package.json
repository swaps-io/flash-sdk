{
  "name": "flash-sdk",
  "version": "1.0.3",
  "description": "Flash SDK",
  "main": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettify": "yarn prettier --write src",
    "p": "yarn prettify",
    "prettify:test": "yarn prettier --write test",
    "pt": "yarn prettify:test",
    "clean": "rm -rf ./dist ./lib",
    "c": "yarn clean",
    "lint": "yarn eslint src",
    "l": "yarn lint",
    "compile": "yarn tsc",
    "co": "yarn compile",
    "mv_lib": "mv ./lib ./dist",
    "cp_src": "cp -r ./src ./dist",
    "build": "yarn clean && yarn lint && yarn compile && yarn mv_lib && yarn cp_src",
    "b": "yarn build",
    "test": "NODE_OPTIONS='--experimental-vm-modules --max-old-space-size=8192' jest --verbose=true --runInBand",
    "t": "yarn test",
    "tg": "yarn test -t",
    "gen:api:main-v0": "orval --config tools/api/main-v0/config.ts",
    "gen:api:crypto-v0": "orval --config tools/api/crypto-v0/config.ts",
    "gen:api:collateral-v0": "orval --config tools/api/collateral-v0/config.ts",
    "gen:api": "yarn gen:api:main-v0 && yarn gen:api:crypto-v0 && yarn gen:api:collateral-v0",
    "ga": "yarn gen:api",
    "docs:gen": "yarn typedoc",
    "dg": "yarn docs:gen",
    "docs:open": "open ./docs/index.html",
    "do": "yarn docs:open",
    "docs": "yarn docs:gen && yarn docs:open",
    "d": "yarn docs",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@safe-global/protocol-kit": "^4.1.1",
    "@tanstack/react-query": "5.56.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.13",
    "@types/qs": "6.9.16",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "axios": "1.7.7",
    "dotenv": "16.4.5",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "ethers": "6.13.2",
    "jest": "29.7.0",
    "orval": "7.1.1",
    "prettier": "3.3.3",
    "qs": "6.13.0",
    "ts-jest": "^29.2.5",
    "typedoc": "0.26.7",
    "typescript": "5.6.2",
    "viem": "2.21.15",
    "wagmi": "2.12.14"
  },
  "peerDependencies": {
    "@safe-global/protocol-kit": ">=4.0 <5",
    "@tanstack/react-query": ">=5.17 <6",
    "axios": ">=1.6 <2",
    "ethers": ">=6.9 <7",
    "qs": ">=6.11 <7",
    "viem": ">=2.1 <3",
    "wagmi": ">=2.2 <3"
  },
  "peerDependenciesMeta": {
    "@safe-global/protocol-kit": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    },
    "ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.5.0"
}
